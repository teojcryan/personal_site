@article{greenwell_simple_2018,
	title = {A {Simple} and {Effective} {Model}-{Based} {Variable} {Importance} {Measure}},
	url = {http://arxiv.org/abs/1805.04755},
	abstract = {In the era of "big data", it is becoming more of a challenge to not only build state-of-the-art predictive models, but also gain an understanding of what's really going on in the data. For example, it is often of interest to know which, if any, of the predictors in a fitted model are relatively influential on the predicted outcome. Some modern algorithms---like random forests and gradient boosted decision trees---have a natural way of quantifying the importance or relative influence of each feature. Other algorithms---like naive Bayes classifiers and support vector machines---are not capable of doing so and model-free approaches are generally used to measure each predictor's importance. In this paper, we propose a standardized, model-based approach to measuring predictor importance across the growing spectrum of supervised learning algorithms. Our proposed method is illustrated through both simulated and real data examples. The R code to reproduce all of the figures in this paper is available in the supplementary materials.},
	urldate = {2020-05-03},
	journal = {arXiv:1805.04755 [cs, stat]},
	author = {Greenwell, Brandon M. and Boehmke, Bradley C. and McCarthy, Andrew J.},
	month = may,
	year = {2018},
	note = {arXiv: 1805.04755},
	keywords = {Computer Science - Machine Learning, Statistics - Machine Learning},
	file = {arXiv Fulltext PDF:C\:\\Users\\admin\\Zotero\\storage\\JTYF2XMC\\Greenwell et al. - 2018 - A Simple and Effective Model-Based Variable Import.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\admin\\Zotero\\storage\\JU2ZEXZR\\1805.html:text/html}
}

@phdthesis{toh_selection_1987,
	type = {Thesis},
	title = {The {Selection} {Criteria} of {HDB} {Resale} {Flats}},
	url = {https://scholarbank.nus.edu.sg/handle/10635/163032},
	abstract = {High-rise, high-density living in flats built by the Housing and Development Board (HDB) is a common phenomenon in Singapore today. The provision of low-cost HDB flats has attracted a high demand for these flats, this being attributed to the fact that the majority of the population find the flat units affordable. To keep pace with the increasing demand, the HDB has stepped up its housing construction programme over the years.
It is, however, interesting to note that the demand for second-hand HDB flats (HDB resale flats) has also been increasing. Although one of the main reasons for the switch to HDB resale flats could be the shortage in supply of new flats in popular localities, other possible reasons cannot be overlooked. The significant role played by the HDB resale market stimulated an interest to make a study into the resale activities of HDB flats.
The HDB resale flats usually command higher prices than the posted prices of new flats. However, purchasers of resale
flats are often prepared to pay for this difference in price. Although price may be a major consideration for many, it is definitely no longer the sole consideration.
The desire to examine other possible factors influencing the purchaser's choice prompted a research into this area by the use of the Statistical Analysis System (SAS) programme.},

}

@phdthesis{tan_determinants_1990,
	type = {Thesis},
	title = {Determinants of {Condominiums} and {HDB} {Resale} {Flat} {Prices}},
	url = {https://scholarbank.nus.edu.sg/handle/10635/165860},
	abstract = {For the past 25 years, Singapore has experienced booms and busts in the residential property market, both the public and private housing properties. Many dissertations have attempted to analyze the determinants of the property prices, but few on the condominiums and none on HDB resale flats. 
This study identifies the determinants of the prices of condominiums and HDB resale flats. The factors that are tested are classified into physical, locational and 
economic factors. From these factors, 17 variables are entered into the condominium model and 12 variables for the HDB resale flats model. 
The analysis of these factors is carried out using the SAS computer package on the IBM 3081 KX2. The Stepwise Multiple Regression Procedure is used to determine the results. 
The dissertation concludes that the physical factors, area and age are the most significant factors affecting both types of property prices. Furthermore, it is 
suggested that over time, locational factors are decreasing in their significance, especially for the condominium model.},

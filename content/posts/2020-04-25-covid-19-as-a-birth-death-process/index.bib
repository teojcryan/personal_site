@article{tritch_duration_2018,
	title = {Duration of a minor epidemic},
	volume = {3},
	issn = {2468-0427},
	url = {http://www.sciencedirect.com/science/article/pii/S2468042717300763},
	doi = {10.1016/j.idm.2018.03.002},
	abstract = {Disease outbreaks in stochastic SIR epidemic models are characterized as either minor or major. When ℛ0{\textless}1, all epidemics are minor, whereas if ℛ0{\textgreater}1, they can be minor or major. In 1955, Whittle derived formulas for the probability of a minor or a major epidemic. A minor epidemic is distinguished from a major one in that a minor epidemic is generally of shorter duration and has substantially fewer cases than a major epidemic. In this investigation, analytical formulas are derived that approximate the probability density, the mean, and the higher-order moments for the duration of a minor epidemic. These analytical results are applicable to minor epidemics in stochastic SIR, SIS, and SIRS models with a single infected class. The probability density for minor epidemics in more complex epidemic models can be computed numerically applying multitype branching processes and the backward Kolmogorov differential equations. When ℛ0 is close to one, minor epidemics are more common than major epidemics and their duration is significantly longer than when ℛ0≪1 or ℛ0≫1.},
	language = {en},
	urldate = {2020-04-21},
	journal = {Infectious Disease Modelling},
	author = {Tritch, William and Allen, Linda J. S.},
	month = jan,
	year = {2018},
	keywords = {Birth-death process, Branching process, Epidemic model, Markov chain},
	pages = {60--73},
	file = {ScienceDirect Snapshot:C\:\\Users\\admin\\Zotero\\storage\\ZW63XNA2\\S2468042717300763.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\admin\\Zotero\\storage\\ZHAE6ZBS\\Tritch and Allen - 2018 - Duration of a minor epidemic.pdf:application/pdf}
}

@article{whittle_outcome_1955,

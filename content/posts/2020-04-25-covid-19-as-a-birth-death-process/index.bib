@article{allen_comparison_2000,
	title = {Comparison of deterministic and stochastic {SIS} and {SIR} models in discrete time},
	volume = {163},
	issn = {0025-5564},
	url = {http://www.sciencedirect.com/science/article/pii/S0025556499000474},
	doi = {10.1016/S0025-5564(99)00047-4},
	abstract = {The dynamics of deterministic and stochastic discrete-time epidemic models are analyzed and compared. The discrete-time stochastic models are Markov chains, approximations to the continuous-time models. Models of SIS and SIR type with constant population size and general force of infection are analyzed, then a more general SIS model with variable population size is analyzed. In the deterministic models, the value of the basic reproductive number R0 determines persistence or extinction of the disease. If R0{\textless}1, the disease is eliminated, whereas if R0{\textgreater}1, the disease persists in the population. Since all stochastic models considered in this paper have finite state spaces with at least one absorbing state, ultimate disease extinction is certain regardless of the value of R0. However, in some cases, the time until disease extinction may be very long. In these cases, if the probability distribution is conditioned on non-extinction, then when R0{\textgreater}1, there exists a quasi-stationary probability distribution whose mean agrees with deterministic endemic equilibrium. The expected duration of the epidemic is investigated numerically.},
	language = {en},
	number = {1},
	urldate = {2020-04-21},
	journal = {Mathematical Biosciences},
	author = {Allen, Linda J. S. and Burgin, Amy M.},
	month = jan,
	year = {2000},
	keywords = {Epidemic, Markov process, Quasi-stationary, Stochastic},
	pages = {1--33},
	file = {ScienceDirect Snapshot:C\:\\Users\\admin\\Zotero\\storage\\26YS85SD\\S0025556499000474.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\admin\\Zotero\\storage\\2D6E95VZ\\Allen and Burgin - 2000 - Comparison of deterministic and stochastic SIS and.pdf:application/pdf}
}

